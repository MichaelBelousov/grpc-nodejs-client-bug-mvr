{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "launch program",
      "skipFiles": ["<node_internals>/**"],
      "program": "node/lib/main.js",
      "preLaunchTask": "npm: build - node",
      "env": {
        "GRPC_TRACE": "all,-timer_check,-timer"
      }
    }
  ]
}
